  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postinstall": "cd ios && pod install && cd ..",
    "test:pre": "cd ./node_modules/react-native-elements/src/config && mv ThemeProvider.js ThemeProvider-root.js && cp ../../../../config/TestHelper/ThemeProvider.js ThemeProvider.js && cd ../../../../",
    "test:after": "cd ./node_modules/react-native-elements/src/config && rm ThemeProvider.js && mv ThemeProvider-root.js ThemeProvider.js",
    "test:handle": "jest",
    "test": "yarn test:pre && yarn test:handle && yarn test:after",
    "test:update": "yarn test:pre && yarn test:handle -- -u && yarn test:after",
    "make:splash": "yarn add -D @bam.tech/react-native-make && react-native set-splash --path src/assets/images/app/splash.png --resize cover --background \"#222222\" && yarn remove @bam.tech/react-native-make",
    "make:icon": "yarn add -D @bam.tech/react-native-make && react-native set-icon --platform ios --path src/assets/images/app/icon-ios.png && react-native set-icon --platform android --path src/assets/images/app/icon-android.png && yarn remove @bam.tech/react-native-make",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings 0"
  },
